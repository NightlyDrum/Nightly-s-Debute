-- Scripts:

local function FUTBOCM_fake_script() -- ScreenGui.UIScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local UIManager = loadstring(game:HttpGet('https://raw.githubusercontent.com/NightlyDrum/Nightly-s-Debute/refs/heads/main/NightlyLib'))()
	local Window = UIManager:CreateWindow("My UI Library")
	
	local Tab1 = Window:MakeTab({
		Name = "Tab 1",
		Callback = function(container)
			UIManager.CreateSlider(container, "Volume", 0, 100, 50)
			UIManager.CreateToggle(container, "Enable Feature", false, function(isEnabled)
				if isEnabled then
					UIManager.CreateNotification("Feature Enabled!", 2)
				else
					UIManager.CreateNotification("Feature Disabled!", 2)
				end
			end)
		end,
	})
	
	local Tab2 = Window:MakeTab({
		Name = "Tab 2",
		Callback = function(container)
			UIManager.CreateSlider(container, "Brightness", 0, 100, 75)
		end,
	})
	
end
coroutine.wrap(FUTBOCM_fake_script)()
